{
    "name": "regnilk/fa-laravel",
    "description": "Laravel component for FontAwesome integration with Blade",
    "type": "library",
    "keywords": ["laravel", "font-awesome", "icon"],
    "license": "MIT",
    "homepage": "https://github.com/regnilk/fa-laravel",
    "authors": [
        {
            "name": "Regnilk",
            "email": "jack@regnilk.fr"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/config": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Regnilk\\FaLaravel\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Regnilk\\FaLaravel\\FaServiceProvider"
            ]
        }
    }
}
